{
  "name": "graphql-playground",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=development tsc-watch --onSuccess 'nodemon ./dist/index.js'",
    "build": "rimraf dist && tsc",
    "lint": "eslint '{src,__tests__}/**/**.{ts,json,js}'",
    "build-schema": "npm-run-all build-schema:*",
    "build-schema:clean": "rimraf src/schema/dist",
    "build-schema:concat": "mkdir src/schema/dist && concat -o src/schema/dist/schema.gql src/schema/*.gql",
    "build-schema:dts": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^1.13.1",
    "@graphql-codegen/typescript": "^1.13.1",
    "@graphql-codegen/typescript-resolvers": "^1.13.1",
    "@types/node-fetch": "^2.5.4",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "apollo-server-express": "~2.6.3",
    "compression": "^1.7.4",
    "concat": "^1.0.3",
    "express": "^4.16.4",
    "graphql": "~14.3.1",
    "graphql-date": "~1.0.3",
    "lodash": "^4.17.15",
    "nodemon": "2.0.2",
    "reflect-metadata": "^0.1.13",
    "replace": "^1.1.5",
    "rimraf": "3.0.2",
    "tsc-watch": "4.2.3",
    "typeorm": "^0.2.24",
    "typescript": "^3.4.5",
    "uuid": "^3.3.2",
    "yarn-run-all": "3.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/graphql": "~14.2.1",
    "@types/graphql-date": "~1.0.3",
    "@types/lodash": "^4.14.123",
    "@types/on-finished": "^2.3.1",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-graphql": "3.1.1",
    "eslint-plugin-typeorm": "0.0.19"
  }
}
